{
  "extends": "airbnb",

  "parser": "babel-eslint",

  "env": {
      "browser": true,
      "node": true
    },

    "plugins": [
      "react"
    ],

  "rules": {
    "linebreak-style": [2, "windows"],
    "semi": [1, "always"],
    "no-debugger": 0,
    "no-console": 1,
    "new-cap": 0,
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "eol-last": 0,
    "comma-dangle": [0, 'always']
  }
}
